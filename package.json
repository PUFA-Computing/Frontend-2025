{
  "name": "puma-web",
  "version": "1.3.12",
  "private": true,
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git",
    [
      "@semantic-release/exec",
      {
        "verifyReleaseCmd": "echo \"NEXT_RELEASE_VERSION=${nextRelease.version}\" >> $GITHUB_ENV"
      }
    ],
    "semantic-release-export-data"
  ],
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "development",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:production": "prisma generate && env-cmd -f .env.production next build",
    "build:staging": "prisma generate && env-cmd -f .env.staging next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --config .prettierrc.json --check '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier:fix": "prettier --config .prettierrc.json --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@designbycode/tailwindcss-text-stroke": "^1.2.1",
    "@headlessui/react": "^2.0.3",
    "@heroicons/react": "^2.0.18",
    "@nextui-org/spinner": "^2.0.28",
    "@nextui-org/theme": "^2.2.3",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-icons/all-files": "^4.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@tiptap/core": "^2.4.0",
    "@tiptap/extension-blockquote": "^2.4.0",
    "@tiptap/extension-bold": "^2.4.0",
    "@tiptap/extension-bullet-list": "^2.4.0",
    "@tiptap/extension-link": "^2.4.0",
    "@tiptap/extension-list-item": "^2.4.0",
    "@tiptap/extension-ordered-list": "^2.4.0",
    "@tiptap/extension-paragraph": "^2.4.0",
    "@tiptap/extension-placeholder": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "form-data": "^4.0.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.344.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.7",
    "next-nprogress-bar": "^2.3.12",
    "nextjs-toploader": "^1.6.4",
    "react": "18.2.0",
    "react-datepicker": "^6.9.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-quill": "^2.0.0",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "sharp": "^0.33.2",
    "sweetalert2": "^11.10.4",
    "swiper": "^11.0.7",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.4",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.10.2",
    "semantic-release": "^23.1.1",
    "semantic-release-export-data": "^1.0.1",
    "tailwindcss": "3.4.3"
  }
}
